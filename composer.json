            "HappyDemon\\EchoServer\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HappyDemon\\EchoServer\\EchoServerServiceProvider"
            ],
            "aliases": {
                "EchoServer": "HappyDemon\\EchoServer\\Facades\\EchoServer",
                "EchoServer": "HappyDemon\\EchoServer\\Facades\\EchoChannels",
                "EchoServer": "HappyDemon\\EchoServer\\Facades\\EchoStorage"
            }
        }
    }
}